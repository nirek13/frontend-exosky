{
    "public": true,
    "rewrites": [
      {
        "source": "/Data/:path*",
        "destination": "/public/Data/:path*"
      },
      {
        "source": "/Textures/:path*",
        "destination": "/public/Textures/:path*"
      },
      {
        "source": "/:path*",
        "destination": "/public/:path*"
      },
      {
        "source": "/api/(.*)",
        "destination": "http://exosky-backend.eastus.cloudapp.azure.com:5000/$1"
      }
    ]
  }
  